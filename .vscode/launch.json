    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File with Arguments",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": ["--decoder_hidden_dim=512", "--encoder_hidden_dim=512", "--kld_weight=0.4", "--lr=1e-2"]
        }
    ]
}